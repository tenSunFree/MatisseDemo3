<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Free." parent="ThemeOverlay.AppCompat">
        <item name="colorButtonNormal">@color/dracula_primary</item>
    </style>

    <style name="Toolbar.Free" parent="ThemeOverlay.AppCompat.Dark.ActionBar">

    </style>

    <style name="Popup.Free" parent="Widget.AppCompat.ListPopupWindow">
        <item name="android:popupBackground">@color/dracula_album_popup_bg</item>
    </style>

    <style name="ImageTranslucentTheme" parent="AppTheme">
        <!--在Android 4.4之前的版本上運行, 直接跟隨系統主題-->
    </style>
</resources>
